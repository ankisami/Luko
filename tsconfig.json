{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "theme/*": ["src/theme/*"],
      "mocks/*": ["src/__mocks__/*"],
      "components": ["src/components"],
      "screens/*": ["src/screens/*"],
      "navigation/*": ["src/navigation/*"],
      "models/*": ["src/models/*"],
      "assets/*": ["src/assets/*"]
    }
  }
}
